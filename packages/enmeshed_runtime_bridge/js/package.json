{
  "name": "js",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --keep-names --target=chrome60,firefox60,safari11,edge18 --external:https-proxy-agent --outdir=../assets --tsconfig=tsconfig.esbuild.json",
    "build:watch": "npm run build -- --watch",
    "lint": "npm run lint:prettier && npm run lint:ts",
    "lint:prettier": "prettier --check .",
    "lint:ts": "tsc --noEmit",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@js-soft/logging-abstractions": "^1.0.1",
    "@js-soft/simple-logger": "^1.0.5",
    "@js-soft/ts-utils": "^2.3.3",
    "@nmshd/app-runtime": "6.59.0",
    "@nmshd/runtime": "6.59.0",
    "js-logger": "^1.6.1",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/json-stringify-safe": "^5.0.3",
    "@types/lodash": "^4.17.20",
    "@types/luxon": "^3.6.2",
    "@types/node": "^24.0.13",
    "esbuild": "^0.25.6",
    "luxon": "^3.7.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  }
}
